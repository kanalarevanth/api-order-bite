{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es2020",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": false,
		"outDir": "dist",
		"rootDir": "src",
		"resolveJsonModule": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules/**/*"]
}
